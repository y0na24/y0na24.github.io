---
import type { HTMLAttributes } from 'astro/types'

interface Props extends HTMLAttributes<'ul'>{}
---

<ul id='dialogParentNode' class:list={['options-list', Astro.props.class]}>
	<slot />
</ul>

<style>
	.options-list {
		display: flex;
		flex-direction: column;
		align-items: center;
		justify-content: center;
		gap: 42px;
		padding-bottom: 96px;
	}
</style>
